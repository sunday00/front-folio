{
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@a/*": ["./src/apis/*"],
      "@v/*": ["./src/views/*"],
      "@c/*": ["./src/components/*"]
    },
    "types": ["module", "vite/client", "three"],
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
  },
  "include": ["src"],
  "references": []
}